{
  "name": "Python Notebook (Codespaces)",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "postCreateCommand": "pip install -r requirements.txt",
  "customizations": {
    "vscode": {
      "settings": {
        "github.copilot.enable": false,
        "github.copilot.inlineSuggest.enable": false
      }
    }
  }
}
